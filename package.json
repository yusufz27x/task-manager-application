{
  "name": "task-manager-application",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm:dev:client\" \"pnpm:dev:server\"",
    "dev:client": "vite",
    "dev:server": "tsx watch api/index.ts",
    "build": "pnpm build:client && pnpm build:server",
    "build:client": "vite build",
    "build:server": "echo 'Build server not implemented yet'",
    "preview": "vite preview",
    "start": "node api/index.js",
    "db:migrate": "pnpm dlx prisma migrate dev",
    "db:generate": "pnpm dlx prisma generate",
    "db:studio": "pnpm dlx prisma studio",
    "db:push": "pnpm dlx prisma db push",
    "db:seed": "pnpm dlx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "@reduxjs/toolkit": "^2.8.2",
    "express": "^5.1.0",
    "mysql2": "^3.14.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-redux": "^7.1.34",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
